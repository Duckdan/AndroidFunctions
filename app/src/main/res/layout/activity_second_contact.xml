<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_first_contact"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="study.com.androidfunctions.contact.FirstContactActivity">

    <com.lqr.recyclerview.LQRRecyclerView
        android:id="@+id/recycler_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:rv_orientation="vertical"
        app:rv_default_animator_open="true"
        app:rv_type="grid"
        />

    <study.com.androidfunctions.weight.IndicatorView
        android:id="@+id/iv"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:layout_gravity="right"
        />

</FrameLayout>

